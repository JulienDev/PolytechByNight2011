<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">

	<include android:layout_width="fill_parent"
		android:layout_height="wrap_content" layout="@layout/en_tete" />

	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:background="@drawable/bg_generique_halo"
		android:orientation="vertical">

		<com.google.android.maps.MapView
			android:id="@+id/mvInfos" android:layout_width="fill_parent"
			android:layout_height="200dp" android:enabled="true"
			android:clickable="true" android:apiKey="@string/gmap" />

		<WebView android:id="@+id/wvInfos" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:background="#00000000"></WebView>

	</LinearLayout>

</LinearLayout>
